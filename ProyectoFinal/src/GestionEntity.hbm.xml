<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Clases.GestionEntity" table="gestion" schema="gestionproyectos">
        <id name="idGestion">
            <column name="Id_gestion" sql-type="int"/>
        </id>
        <property name="cdProveedor">
            <column name="CDProveedor" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="cdPieza">
            <column name="CDPieza" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="cdProyecto">
            <column name="CDProyecto" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="cantidad">
            <column name="cantidad" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>